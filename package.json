{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "engines": {
    "node": "16.16.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "postbuild": "next-sitemap",
    "start": "next start -p $PORT",
    "start:dev": "next start",
    "heroku-postbuild": "npm run build",
    "test": "echo \"No heroku test script\" && exit 1",
    "lint": "next lint && npx eslint .",
    "build:css": "postcss styles/tailwind.css -o styles/main.css",
    "build:watch": "postcss styles/tailwind.css -o styles/main.css --watch",
    "prod:build": "NODE_ENV=production && npm run build:css && npm run build",
    "ci:build": "npm run build:css && npm run build",
    "prettier": "prettier -c '**/*.{ts,tsx,js,jsx,css,less,json}'",
    "prettier:write": "prettier -w '**/*.{ts,tsx,js,jsx,css,less,json}'",
    "prismamigrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prismamigrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "prismastudio:main": "dotenv -e .env.local -- npx prisma studio",
    "prismastudio:test": "dotenv -e .env.test -- npx prisma studio",
    "pretest": "dotenv -e .env.test -- npx prisma db seed",
    "build:test": "dotenv -e .env.test -- next build",
    "test:playwright": "dotenv -e .env.test -- playwright test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:local": "npm run docker:up && npm run prismamigrate:test && npm run pretest && npm run test:playwright && npm run docker:down",
    "test:record": "npm run docker:up && npm run prismamigrate:test && npm run pretest && dotenv -e .env.test -- next start",
    "test:stop": "npm run docker:down",
    "test:remote": "npm run pretest && npm run build:test && npm run test:playwright"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.15.2",
    "@sentry/nextjs": "^7.6.0",
    "@sentry/webpack-plugin": "^1.18.9",
    "@types/nodemailer": "^6.4.4",
    "@types/underscore": "^1.11.4",
    "autoprefixer": "^10.4.7",
    "aws-sdk": "^2.1176.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "cssnano": "^5.1.12",
    "daisyui": "^2.17.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "fast-average-color": "^9.1.1",
    "i18next": "^21.8.11",
    "multer": "^1.4.5-lts.1",
    "next": "^12.1.6",
    "next-auth": "^4.8.0",
    "next-sitemap": "^3.1.7",
    "next-themes": "^0.2.0",
    "nodemailer": "^6.7.7",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.3",
    "react-material-ui-carousel": "^3.4.2",
    "react-promise-tracker": "^2.1.0",
    "react-toastify": "^9.0.5",
    "sharp": "^0.30.7",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.4",
    "underscore": "^1.13.4",
    "usehooks-ts": "^2.6.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.2.3",
    "@playwright/test": "^1.24.0",
    "@types/node": "^18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.18.0",
    "eslint-config-next": "12.1.6",
    "prisma": "^3.15.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  }
}
